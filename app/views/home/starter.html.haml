                    %li
                      %a{:href => "#"} Themes
                    %li
                      %a{:href => "#"} Language
                .col-6.pe-1.ps-1
                  %ul.list-unstyled
                    %li
                      %a{:href => "#"} Devices
                    %li
                      %a{:href => "#"} Storage
