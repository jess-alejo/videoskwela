              %button.plyr__controls__item.plyr__control{"aria-label" => "Play", "data-plyr" => "play", :type => "button"}
                %svg.icon--pressed{"aria-hidden" => "true", :focusable => "false"}
                  %use{"xlink:href" => "#plyr-pause"}
                %svg.icon--not-pressed{"aria-hidden" => "true", :focusable => "false"}
                  %use{"xlink:href" => "#plyr-play"}
                %span.label--pressed.plyr__sr-only Pause
                %span.label--not-pressed.plyr__sr-only Play
              .plyr__controls__item.plyr__progress__container
                .plyr__progress
                  %input#plyr-seek-6490{"aria-label" => "Seek", "aria-valuemax" => "100", "aria-valuemin" => "0", "aria-valuenow" => "0", "aria-valuetext" => "00:00 of 03:03", :autocomplete => "off", "data-plyr" => "seek", :max => "100", :min => "0", :role => "slider", :step => "0.01", :style => "--value:0%;", :type => "range", :value => "0"}/
                  %progress.plyr__progress__buffer{"aria-hidden" => "true", :max => "100", :min => "0", :role => "progressbar", :value => "0"} % buffered
                  %span.plyr__tooltip 00:00
              .plyr__controls__item.plyr__time--current.plyr__time{"aria-label" => "Current time"} -03:03
              .plyr__controls__item.plyr__volume
                %button.plyr__control{"data-plyr" => "mute", :type => "button"}
                  %svg.icon--pressed{"aria-hidden" => "true", :focusable => "false"}
                    %use{"xlink:href" => "#plyr-muted"}
                  %svg.icon--not-pressed{"aria-hidden" => "true", :focusable => "false"}
                    %use{"xlink:href" => "#plyr-volume"}
                  %span.label--pressed.plyr__sr-only Unmute
                  %span.label--not-pressed.plyr__sr-only Mute
                %input#plyr-volume-6490{"aria-label" => "Volume", "aria-valuemax" => "100", "aria-valuemin" => "0", "aria-valuenow" => "100", "aria-valuetext" => "100.0%", :autocomplete => "off", "data-plyr" => "volume", :max => "1", :min => "0", :role => "slider", :step => "0.05", :style => "--value:100%;", :type => "range", :value => "1"}/
