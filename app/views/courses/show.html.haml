- page_title @course.title
- course_presenter = CoursePresenter.new(@course)

.container
  / Title and Top Buttons Start
  .page-title-container
    .row
      / Title Start
      .col-12.col-sm-6
        %h1#title.mb-0.pb-0.display-4
          = @course.title
        %nav.breadcrumb-container.d-inline-block{"aria-label" => "breadcrumb"}
          %ul.breadcrumb.pt-0
            %li.breadcrumb-item
              = link_to "Home", root_path
            %li.breadcrumb-item
              = link_to "Courses", courses_path
      / Title End
      / Top Buttons Start
      .col-12.col-sm-6.d-flex.align-items-start.justify-content-end
        / Start Now Start
        %a.btn.btn-primary.btn-icon.btn-icon-start.w-100.w-sm-auto{:href => "Misc.Player.html"}
          %i{ data: { "acorn-icon" => "chevron-right" } }
          %span Start Now
        / Start Now End
      / Top Buttons End
  / Title and Top Buttons End
  / Content Start
  .row
    / Left Side Start
    .col-12.col-xxl-8.mb-5
      / Preview Start
      %h2.small-title Preview
      .card.mb-5
        .card-img-top
          .plyr.plyr--full-ui.plyr--video.plyr--html5.plyr--fullscreen-enabled.plyr--paused.plyr--stopped.plyr--pip-supported.plyr__poster-enabled{:tabindex => "0"}
